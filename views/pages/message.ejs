<%- include('../partials/header') %>
<% messages.forEach((message) => { %>
  <% console.log(message.id, param.id) %> 
  <% if (message.id == param.id) { %>
    <h4> User: <%= message.user %></h4>
    <p> Message: <%= message.text %></p>
    <p> Time: <%= message.added %></p>
  <% } else {%>
    <% return %>
<% }}); %>
